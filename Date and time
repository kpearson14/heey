from datetime import datetime
now= datetime.now()
print now
currenthour=now.hour
currentminute=now.minute
currentsecond=now.second
currentyear= now.year
currentmonth= now.month
currentday= now.day
print(str(currentday) +"/"+ str(currentmonth)+ "/"+ str(currentyear) +" " + str(currenthour) + ":"+ str(currentminute) +":" +str(currentsecond))
